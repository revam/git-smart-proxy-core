{
  "name": "git-packet-streams",
  "version": "1.1.0",
  "description": "Helper streams for to work with the git packet format",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tslint test/**/*.ts && jest",
    "build": "tsc --noEmit -p . && tslint src/**/*.ts && tsc -p .",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revam/node-git-service.git"
  },
  "keywords": [
    "git",
    "packet",
    "pack-format",
    "typescript"
  ],
  "author": "Mikal Stordal <mikalstordal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/revam/node-git-service/issues"
  },
  "homepage": "https://github.com/revam/node-git-service#readme",
  "devDependencies": {
    "del-cli": "^1.1.0",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
