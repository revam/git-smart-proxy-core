{
  "name": "git-service-driver",
  "version": "1.0.0",
  "description": "Basic git service driver",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tslint test/**/*.ts && jest",
    "build": "tsc --noEmit -p . && tslint src/**/*.ts && tsc -p .",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revam/node-git-service.git"
  },
  "keywords": [
    "git",
    "git-service",
    "driver",
    "typescript"
  ],
  "author": "Mikal Stordal <mikalstordal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/revam/node-git-service/issues"
  },
  "homepage": "https://github.com/revam/node-git-service#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.8",
    "git-service": "^1.0.0",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.5",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "node-fetch": "^2.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
