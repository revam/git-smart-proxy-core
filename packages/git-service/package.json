{
  "name": "git-service",
  "version": "1.0.0",
  "description": "Serve git over http(s)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "tsc --noEmit -p . && tslint src/**/*.ts",
    "test": "jest",
    "build:release": "tsc -p .",
    "build:docs": "typedoc --out ./docs/ --mode modules --excludePrivate --excludeProtected --readme DOCS.md --tsconfig ./tsconfig.json",
    "pretest": "npm run lint",
    "prebuild:release": "npm run test",
    "prebuild:docs": "del-cli docs",
    "prepublish": "npm run build:release",
    "postpublish": "del-cli lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/revam/node-git-service.git"
  },
  "keywords": [
    "git",
    "push",
    "pull",
    "deploy",
    "typescript"
  ],
  "author": "Mikal Stordal <mikalstordal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/revam/node-git-service/issues"
  },
  "homepage": "https://github.com/revam/node-git-service#readme",
  "devDependencies": {
    "@types/http-status": "^0.2.30",
    "@types/jest": "^22.0.0",
    "@types/node": "^8.5.2",
    "@types/node-fetch": "^1.6.7",
    "@types/through": "0.0.29",
    "del-cli": "^1.1.0",
    "dotenv": "^5.0.1",
    "http-status": "^1.0.1",
    "into-stream": "^3.1.0",
    "jest": "^22.0.4",
    "tempy": "^0.2.1",
    "through": "^2.3.8",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "git-packet-streams": "^1.0.0",
    "git-side-band-message": "0.0.3",
    "micro-signals": "^2.0.0",
    "node-fetch": "^2.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "packages"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
